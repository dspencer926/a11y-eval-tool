{
  "name": "a11y-eval-tool",
  "version": "0.1.0",
  "description": "Automatic accessibility testing with the WAVE API",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack": "NODE_ENV=production webpack -vpc --config webpack.config.js",
    "build": "npm run clean && npm run build:webpack",
    "start": "node devServer.js",
    "lint": "eslint 'src/**/*.@(js|jsx)'",
    "test": "NODE_ENV=test mocha --compilers js:babel-core/register -r test/.setup.js --recursive ./test",
    "test:watch": "NODE_ENV=test mocha -w --compilers js:babel-core/register -r test/.setup.js --recursive ./test",
    "test:watch-min": "NODE_ENV=test mocha -w --compilers js:babel-core/register --reporter min -r test/.setup.js --recursive ./test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vga-/a11y-eval-tool.git"
  },
  "keywords": [
    "a11y",
    "WAVE",
    "accessibility"
  ],
  "author": "Vincent Abruzzo, Hampshire College Web Apps and Services",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/vga-/a11y-eval-tool/issues"
  },
  "homepage": "https://github.com/vga-/a11y-eval-tool#readme",
  "devDependencies": {
    "async.map": "^0.5.2",
    "autoprefixer": "^6.3.6",
    "babel-loader": "^6.2.4",
    "babel-preset-airbnb": "^1.1.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.7.2",
    "browser-filesaver": "^1.1.1",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.4.2",
    "css-loader": "^0.23.1",
    "enzyme": "^2.2.0",
    "es6-promise": "^3.1.2",
    "eslint": "^2.8.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-plugin-import": "^1.6.0",
    "eslint-plugin-jsx-a11y": "^1.0.3",
    "eslint-plugin-react": "^5.0.1",
    "exports-loader": "^0.6.3",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "he": "^1.0.0",
    "html-webpack-plugin": "^2.16.0",
    "ignore-styles": "^2.0.0",
    "jsdom": "^8.4.1",
    "json-loader": "^0.5.4",
    "lodash.map": "^4.3.0",
    "mocha": "^2.4.5",
    "moment": "^2.13.0",
    "postcss-loader": "^0.8.2",
    "precss": "^1.4.0",
    "react": "^15.0.1",
    "react-addons-test-utils": "^15.0.1",
    "react-bootstrap": "^0.29.3",
    "react-dom": "^15.0.1",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.3",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0",
    "whatwg-fetch": "^0.11.0"
  }
}
